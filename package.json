    "gulp": "^3.9.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "through2": "^2.0.0",
    "vinyl-source-stream": "^1.1.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
