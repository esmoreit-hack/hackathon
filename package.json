{
  "name": "esmoreit-chess-stellar",
  "version": "1.0.0",
  "description": "Esmoreit-chess-stellar",
  "main": "index.js",
  "scripts": {
    "start": "npm run deploy && npm server-dev",
    "start-prod": "export NODE_ENV=production && npm run deploy && npm server",
    "server": "node_modules/.bin/pm2 start -f ./src/server",
    "server-dev": "node_modules/.bin/pm2 stop ./src/server && node_modules/.bin/pm2 start ./src/server --watch",
    "dev": "npm run server-dev && npm run watch-js",
    "server-wp": "./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --port 8081 --config webpack.config.js --progress --colors",
    "watch-js": "./node_modules/.bin/webpack --config webpack.config.js --watch",
    "build-assets": "./node_modules/.bin/webpack -p --config webpack.config.js",
    "build-map": "node src/libs/utils/MapGenerator.js --harmony_destructuring",
    "deploy": "npm run build-map && npm run build-assets"
  },
  "keywords": [
    "ES6",
    "Babel",
    "Babel 6",
    "Webpack"
  ],
  "contributors": [
    {
      "name": "Mauro Mandracchia",
